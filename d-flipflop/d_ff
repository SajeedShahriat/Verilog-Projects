#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x998fbe8 .scope module, "d_ff_tb" "d_ff_tb" 2 8;
 .timescale 0 0;
v0x998c498_0 .var "clk", 0 0;
v0x998c518_0 .var "data", 0 0;
v0x998c588_0 .net "q", 0 0, v0x998c3d0_0; 1 drivers
v0x998c5f8_0 .var "reset", 0 0;
S_0x996c3e0 .scope module, "connect_d_ff" "d_ff" 2 41, 3 8, S_0x998fbe8;
 .timescale 0 0;
v0x996c460_0 .net "clk", 0 0, v0x998c498_0; 1 drivers
v0x998c370_0 .net "data", 0 0, v0x998c518_0; 1 drivers
v0x998c3d0_0 .var "q", 0 0;
v0x998c430_0 .net "reset", 0 0, v0x998c5f8_0; 1 drivers
E_0x998e508 .event posedge, v0x996c460_0;
    .scope S_0x996c3e0;
T_0 ;
    %wait E_0x998e508;
    %load/v 8, v0x998c430_0, 1;
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_0.0, 4;
    %ix/load 0, 1, 0;
    %assign/v0 v0x998c3d0_0, 0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0x998c370_0, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v0x998c3d0_0, 0, 8;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x998fbe8;
T_1 ;
    %delay 100, 0;
    %load/v 8, v0x998c498_0, 1;
    %inv 8, 1;
    %set/v v0x998c498_0, 8, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x998fbe8;
T_2 ;
    %vpi_call 2 21 "$dumpfile", "d_ff_wave.vcd";
    %vpi_call 2 22 "$dumpvars", 1'sb0, S_0x998fbe8;
    %vpi_call 2 24 "$display", "time\011 clk reset data q";
    %vpi_call 2 25 "$monitor", "%g\011 %b %b %b", $time, v0x998c498_0, v0x998c5f8_0, v0x998c518_0, v0x998c588_0;
    %set/v v0x998c498_0, 1, 1;
    %set/v v0x998c518_0, 0, 1;
    %set/v v0x998c5f8_0, 0, 1;
    %delay 100, 0;
    %set/v v0x998c518_0, 1, 1;
    %delay 300, 0;
    %set/v v0x998c5f8_0, 1, 1;
    %delay 300, 0;
    %set/v v0x998c5f8_0, 0, 1;
    %delay 600, 0;
    %set/v v0x998c518_0, 0, 1;
    %delay 500, 0;
    %set/v v0x998c518_0, 1, 1;
    %delay 200, 0;
    %set/v v0x998c518_0, 0, 1;
    %delay 400, 0;
    %vpi_call 2 37 "$finish";
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "d-ff_tb.v";
    "d-ff.v";
